#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/Hevellyn/behappy-frontend/node_modules/esprima/bin/esvalidate.js/node_modules:/home/aluno/Hevellyn/behappy-frontend/node_modules/esprima/bin/node_modules:/home/aluno/Hevellyn/behappy-frontend/node_modules/esprima/node_modules:/home/aluno/Hevellyn/behappy-frontend/node_modules:/home/aluno/Hevellyn/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/Hevellyn/behappy-frontend/node_modules/esprima/bin/esvalidate.js/node_modules:/home/aluno/Hevellyn/behappy-frontend/node_modules/esprima/bin/node_modules:/home/aluno/Hevellyn/behappy-frontend/node_modules/esprima/node_modules:/home/aluno/Hevellyn/behappy-frontend/node_modules:/home/aluno/Hevellyn/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../esprima/bin/esvalidate.js" "$@"
  ret=$?
fi
exit $ret
